{
  "name": "kokspflanze/p-server-core",
  "description": "Zendframework 3 Module for pServer ",
  "keywords": [
    "framework",
    "zf2",
    "game backend",
    "p-server-cms"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kokspflanze/PServerCore",
  "autoload": {
    "psr-4": {
      "PServerCore\\": "src/PServerCore"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PServerCoreTest\\": "tests/PServerCoreTest"
    }
  },
  "authors": [
    {
      "name": "Oliver",
      "email": "kokspflanze@protonmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-intl" : "*",
    "ext-gd" : "*",
    "ext-curl" : "*",
    "ext-json" : "*",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-mvc": "^3.1.0",
    "zendframework/zend-escaper": "^2.5.2",
    "zendframework/zend-view": "^2.9.0",
    "zendframework/zend-i18n": "^2.7.4",
    "zendframework/zend-session": "^2.8.0",
    "zendframework/zend-http": "^2.6.0",
    "zendframework/zend-hydrator": "^2.2.2",
    "zendframework/zend-cache": "^2.7.2",
    "zendframework/zend-stdlib": "^3.1.0",
    "zendframework/zend-form": "^2.10.2",
    "zendframework/zend-inputfilter": "^2.7.4",
    "zendframework/zend-validator": "^2.10.1",
    "zendframework/zend-mail": "^2.8.0",
    "zendframework/zend-mime": "^2.6.1",
    "zendframework/zend-router": "^3.0.2",
    "zendframework/zend-serializer": "^2.8",
    "zendframework/zend-log": "^2.9.2",
    "zendframework/zend-navigation": "^2.8.2",
    "doctrine/doctrine-orm-module": "^1.1.3 | ^2.1",
    "kokspflanze/game-backend": "~0.9",
    "kokspflanze/payment-api": "^1.5.0",
    "kokspflanze/zfc-bbcode": "^2.1.1",
    "san/san-captcha": "^1.0.0",
    "kokspflanze/small-user": "^2.0.0",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "kokspflanze/zfc-ticket-system": "~0.5",
    "kokspflanze/zfc-twig": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.6"
  },
  "scripts": {
    "test": "phpunit --configuration tests/phpunit.xml --colors=always",
    "test-coverage": "phpunit --configuration tests/phpunit.xml --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v"
  }
}
