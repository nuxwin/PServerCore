{
  "name": "kokspflanze/p-server-core",
  "description": "Zendframework 3 Module for pServer ",
  "keywords": [
    "framework",
    "zf2",
    "game backend",
    "p-server-cms"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kokspflanze/PServerCore",
  "autoload": {
    "psr-4": {
      "PServerCore\\": "src/PServerCore"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PServerCoreTest\\": "tests/PServerCoreTest"
    }
  },
  "authors": [
    {
      "name": "Oliver",
      "email": "kokspflanze@protonmail.com"
    }
  ],
  "require": {
    "php": "^7.0.6",
    "ext-intl" : "*",
    "ext-gd" : "*",
    "ext-curl" : "*",
    "ext-json" : "*",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-mvc": "^3.0",
    "zendframework/zend-escaper": "^2.5",
    "zendframework/zend-view": "^2.5",
    "zendframework/zend-i18n": "^2.5",
    "zendframework/zend-session": "^2.5",
    "zendframework/zend-http": "^2.5",
    "zendframework/zend-hydrator": "^2.1",
    "zendframework/zend-cache": "^2.5",
    "zendframework/zend-stdlib": "^3.0",
    "zendframework/zend-form": "^2.5",
    "zendframework/zend-inputfilter": "^2.5",
    "zendframework/zend-validator": "^2.5",
    "zendframework/zend-mail": "^2.5",
    "zendframework/zend-mime": "^2.5",
    "zendframework/zend-router": "^3.0",
    "zendframework/zend-serializer": "^2.5",
    "zendframework/zend-log": "^2.5",
    "doctrine/doctrine-orm-module": "^1.1.1",
    "kokspflanze/game-backend": "~0.6",
    "kokspflanze/payment-api": "^1.4",
    "kokspflanze/zfc-bbcode": "^2.1",
    "san/san-captcha": "^0.0.4",
    "kokspflanze/small-user": "^1.0",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "kokspflanze/zfc-ticket-system": "~0.3",
    "kokspflanze/zfc-twig": "^1.3 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.6",
    "satooshi/php-coveralls": "dev-master"
  }
}
